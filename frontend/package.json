{
    "name": "rent-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "type-check": "tsc --noEmit",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "cross-env NEXT_PUBLIC_MOCK_MODE=true playwright test",
        "test:e2e:ui": "cross-env NEXT_PUBLIC_MOCK_MODE=true playwright test --ui",
        "test:e2e:real": "cross-env NEXT_PUBLIC_MOCK_MODE=false playwright test"
