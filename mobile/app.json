{
  "expo": {
    "name": "rent",
    "slug": "rent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.maese.rent"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "rent",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "237cd0d5-23f5-49d5-88d7-3c7858833e46"
      }
    },
    "owner": "acastiglia"
  }
}
